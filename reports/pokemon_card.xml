<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="report_pokemon_cards">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <h2>Pokemon Card</h2>
            </t>
        </t>
    </template>

    <template id="report_pokemon">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="pokedex.report_pokemon_cards" />
            </t>
        </t>
    </template>


</odoo>